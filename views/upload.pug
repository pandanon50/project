extends layouts/main.pug

block content
    .form-container
        form(action=`/meet${routes.upload}`,method="post",enctype="multipart/form-data")
            label(for="file") Img File
            input(type="file" ,id="file",name="imgFile",accept="img/*")
            input(type="text", placeholder="Title",name="title",required="true")
            input(type="submit",value="Upload meet")
